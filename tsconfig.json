{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "public",
    "strict": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true, 
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "DB/*" : ["database/*"]
    },
  },
  "include": ["src/**/*", "views/_app.config.js", "src/routes/pixiv","app.ts"],
  "exclude": ["node_modules", "public","src/routes/index.ts"]
}
